<?xml version="1.0"?>
<!DOCTYPE tsung SYSTEM "/home/ec2-user/tsung-1.5.0/tsung-1.0.dtd">
<tsung loglevel="notice" version="1.0" dumptraffic="false">
  <!-- Client side setup -->
  <clients>
    <client host="localhost" use_controller_vm="true" maxusers='15000'/>
  </clients>

  <servers>
    <server host="ec2-52-33-123-54.us-west-2.compute.amazonaws.com" port="80" type="tcp"></server>
  </servers>

  <load>
    <arrivalphase phase="1" duration="20" unit="second">
      <users arrivalrate="10" unit="second"></users>
    </arrivalphase>
  </load>

  <options>
    <option name="global_ack_timeout" value="2000"></option>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>
  
  <sessions>
    <session name='login' probability='100' type='ts_http'>
      <request subst="true">
        <http url="/login" version="1.1" method="POST" contents="utf8=%E2%9C%93&amp;session%5Bemail%5D=a%40a&amp;session%5Bpassword%5D=a&amp;commit=Log+in"></http>
      </request>
    </session>
  </sessions>
</tsung>
